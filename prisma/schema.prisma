generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model school {
  id       Int    @id @default(autoincrement())
  name     String
  address  String
  city     String
  state    String
  contact  BigInt
  email_id String
  image    String
}
